  "contributors": [
    {
      "name": "Alexander Zakharov (azaxarov)",
      "email": "alexander.zakharov@bouvet.com"
    },
    {
      "name": "Jeff Reiffers",
      "email": "jeff.reiffers@digdir.no"
    }
